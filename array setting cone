int state=0;
int flag = 0;
int const nilai=5;
int setting[nilai];
void setup() {
  Serial1.begin(9600);
  Serial.begin(9600);
}
void loop() {
  if(Serial1.available()){
    state = Serial1.read();
    setting[flag]=state;
    if(state==7){
      flag=-1;
    }
    flag++;
    delay(200);
    if(flag==5){
      for(int i=0;i<5;i++){
        Serial.println(setting[i]);
      }
    flag=0;
    }
  }
}
